[
 {
    "name": "front-container",
    "image": "827438519178.dkr.ecr.ap-northeast-1.amazonaws.com/lifree-front:latest",
    "essential": true,
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-region": "ap-northeast-1",
            "awslogs-stream-prefix": "front",
            "awslogs-group": "/ecs/front"
        }
    },
    "portMappings": [
        {
            "protocol": "tcp",
            "containerPort": 80
        }
    ],
    "command": [
        "yarn", "run", "dev"
    ],
    "environment": [
      {
        "name": "NODE_ENV",
        "value": "production"
      },
      {
        "name": "WORKDIR",
        "value": "app"
      },
      {
        "name": "HOST",
        "value": "0.0.0.0"
      },
      {
        "name": "PORT",
        "value": "80"
      }
    ]
 }    
]